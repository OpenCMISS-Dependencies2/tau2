#!/bin/sh

PERFDB_JAR=${PERFDB_HOME}/jars/perfdb.jar
XMLSAX_JAR=${PERFDB_HOME}/jars/xerces.jar
JDBC_JAR=${PERFDB_HOME}/jars/postgresql.jar

java -cp ${PERFDB_JAR}:${XMLSAX_JAR}:${JDBC_JAR} perfdb.loadxml.Main ${PERFDB_HOME}/data/perfdb.cfg loadexp 1 ${PERFDB_HOME}/data/Sys_Info.xml  ${PERFDB_HOME}/data/Config_Info.xml  ${PERFDB_HOME}/data/Compiler_Info.xml  ${PERFDB_HOME}/data/Instru_Info.xml 
