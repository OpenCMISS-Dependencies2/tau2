<?xml version="1.0" encoding="iso-8859-1"?>


<!-- @version: -->

<Problem_Specification>

   <Meta>
       <title>Sample Problem Specification</title>
   </Meta>
    
    
    <!--____________________________________________________________________-->
    <!--      T  I  M  E     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <Time>
       <maxTime>            1.0       </maxTime>
       <initTime>           0.0         </initTime>
       <delt_min>           0.0         </delt_min>
       <delt_max>           1.0         </delt_max>
       <delt_init>          1.0e-4      </delt_init>
       <max_iterations>      30         </max_iterations>
       <timestep_multiplier>0.8         </timestep_multiplier>
   </Time>
   
    <!--____________________________________________________________________-->
    <!--      G  R  I  D     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
    <Grid>
    <BoundaryConditions>
      <Face side = "x-">
         <BCType id = "0" var = "symmetry" label = "Symmetric">
        </BCType>
      </Face>
      <Face side = "x+">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" label = "Velocity"     var = "Dirichlet">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" label = "Temperature"  var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" label = "Density"      var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
      </Face>
      <Face side = "y-">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" label = "Velocity"     var = "Dirichlet">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" label = "Temperature"  var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" label = "Density"      var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
      </Face>                  
      <Face side = "y+">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" label = "Velocity"     var = "Neumann">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" label = "Temperature"  var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" label = "Density"      var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
      </Face>
      <Face side = "z-">
        <BCType id = "0" var = "symmetry" label = "Symmetric">
        </BCType>
        <BCType id = "0" label = "Velocity"     var = "Dirichlet">
                              <value> [0.,0.,0.] </value>
        </BCType>
      </Face>                  
      <Face side = "z+">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" label = "Velocity"     var = "Dirichlet">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" label = "Temperature"  var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" label = "Density"      var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
      </Face>
    </BoundaryConditions>
       <Level>
           <Box label="1">
              <lower>        [0.0, -0.1,0.0]   </lower>
              <upper>        [0.1,  0.2,0.1]   </upper>
              <extraCells>   [1,1,1]            </extraCells>
              <patches>      [2,2,2]            </patches>
              <resolution>   [30, 90, 30]          </resolution>
           </Box>
           
       </Level>
    </Grid>
   
    <!--____________________________________________________________________-->
    <!--   O  U  P  U  T     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <DataArchiver>
      <filebase>jet_Cu_Cylinder.uda</filebase>
       <outputInterval>0</outputInterval>
      <save label="press_equil_CC"/>
      <save label="vol_frac_CC"/>
      <save label="delP_Dilatate"/>
      <save label="press_CC"/>
      <save label="temp_CC"/>
      <save label="rho_CC"/>
      <save label="vel_CC"/>
      <save label="p.x"/>
      <save label="p.volume"/>
      <save label="p.temperature"/>
   </DataArchiver>

    <!--____________________________________________________________________-->
    <!--    I  C  E     P  A  R  A  M  E  T  E  R  S                        -->
    <!--____________________________________________________________________-->
    <CFD>
       <cfl>0.5</cfl>
       <ICE>
        <max_iteration_equilibration> 50 </max_iteration_equilibration>
        <advection type = "SecondOrderCE" />
        <solution technique = "EqForm" />
      </ICE>        
    </CFD>
    <MPM>
        <time_integrator>explicit</time_integrator>
        <nodes8or27>8</nodes8or27>
    </MPM>

    <!--____________________________________________________________________-->
    <!--     P  H  Y  S  I  C  A  L     C  O  N  S  T  A  N  T  S           -->
    <!--____________________________________________________________________-->   
    <PhysicalConstants>
       <gravity>            [0,0,0]   </gravity>
       <reference_pressure> 101325.0  </reference_pressure>
    </PhysicalConstants>


    <!--____________________________________________________________________-->
    <!--    MATERIAL PROPERTIES INITIAL CONDITIONS                          -->
    <!--____________________________________________________________________-->
    <MaterialProperties>
       <MPM>
         <material name = "Copper cylinder">
                <burn type = "null"/>
               <EOS type = "ideal_gas">
                  <gas_constant>      287               </gas_constant>
               </EOS>
                <density>             8900.0         </density>
              <constitutive_model type = "comp_neo_hook_plastic">
                  <useModifiedEOS>     true          </useModifiedEOS>
                  <bulk_modulus>      117.0e7        </bulk_modulus>
                  <shear_modulus>      43.8e7        </shear_modulus>
                  <yield_stress>       70.0e6        </yield_stress>
                  <hardening_modulus>  43.8e5        </hardening_modulus>
                  <alpha>               0.0          </alpha>
                  <strain_limit>       3.05          </strain_limit>
                  <reduction_factor>   1.0           </reduction_factor>
              </constitutive_model>
              <toughness>               10.e6         </toughness>
                <thermal_conductivity>  400.0         </thermal_conductivity>
                <specific_heat>         386.0         </specific_heat>
               <gamma>                  1.4           </gamma>           
                <geom_object>
                   <difference>
                    <cylinder label = "outer cylinder">
                     <bottom>           [0.0,0.0,-.05715]   </bottom>
                     <top>              [0.0,0.0, .05715]   </top>
                     <radius>           0.05715            </radius>
                   </cylinder>
                   <cylinder label = "inner cylinder">
                     <bottom>           [0.0,0.0,-.0508]   </bottom>
                     <top>              [0.0,0.0, .0508]   </top>
                     <radius>           0.0508             </radius>
                   </cylinder>
                   </difference>
                 <res>                 [2,2,2]         </res>
                 <velocity>            [0.0,0.0,0.0]   </velocity>
                 <temperature>         300             </temperature>
                </geom_object>
         </material>

            <contact>
              <type>null</type>
              <vel_fields>              [0,0,0]         </vel_fields>
            </contact>
      </MPM>
       <ICE>
         <material>
            <burn type = "null" />       
           <EOS type = "ideal_gas">
              <gas_constant>            287             </gas_constant>
           </EOS>
           <viscosity>                  1e-6            </viscosity>
           <thermal_conductivity>       .026            </thermal_conductivity>
           <specific_heat>              716.0           </specific_heat>
           <speed_of_sound>             376             </speed_of_sound>
           <gamma>                      1.4             </gamma>           
           <geom_object>
             <union>
                <difference>
                  <box label="domain">
                      <min>           [-1.1,-1.1,-1.1]    </min>
                      <max>           [ 1.1, 1.1, 1.1 ]   </max>
                  </box>
                  
                  <cylinder label = "container">
                     <bottom>       [0.0,0.0,-.05715]     </bottom>
                     <top>          [0.0,0.0, .05715]     </top>
                     <radius>       0.05715               </radius>
                   </cylinder>
                </difference>
                <cylinder label = "inside the  cylinder">
                      <bottom>        [0.0,0.0,-.0508]    </bottom> 
                      <top>           [0.0,0.0, .0508]    </top>    
                      <radius>        0.0508              </radius> 
                </cylinder>
            </union>
            <res>                      [2,2,2]        </res>
            <velocity>           [0.0,0.0,0.0]       </velocity>
            <temperature>        300.0               </temperature>
            <density>    1.1792946927374306000e+00   </density>
            <pressure>           101325.0            </pressure>
           </geom_object>
         </material>
      </ICE>       
       
            <exchange_properties> 
         <exchange_coefficients>
              <momentum>               [1e10]          </momentum>
              <heat>                   [1e10]          </heat>
        </exchange_coefficients>
            </exchange_properties> 
    </MaterialProperties>
            

    <PhysicalBC>
       <MPM>
      </MPM>
    </PhysicalBC>
       
</Problem_specification>
