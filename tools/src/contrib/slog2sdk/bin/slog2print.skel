#!/bin/@SHELL@
TAUROOT=@TAUROOTDIR@
MACHINE=@ARCH@
LIBDIR=${TAUROOT}/${MACHINE}/lib

# MAIN_LIB=${GUI_LIBDIR}/slog2printrecur.jar
MAIN_LIB=${LIBDIR}/slog2printserial.jar


if [ -z "${GUI_LIBDIR}" -o "${GUI_LIBDIR}" = "/lib" ]; then
    echo "GUI_LIBDIR is NOT set!"
    exit 1
else
    echo "GUI_LIBDIR is set. GUI_LIBDIR = "${GUI_LIBDIR}
fi

java -Xmx500m -Xms32m -jar @JPATH_START@${MAIN_LIB}@JPATH_FINAL@ "$@"
